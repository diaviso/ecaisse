{% extends 'base.html.twig' %}

{% block title %}Ticket{% endblock %}

{% block body %}
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="panel-title">
                    <h3>Visualisation Ticket</h3>
                </div>
            </div>
            <div class="panel-body">
                <table class="table">
                    <tbody>
                                                    <tr>
                                <th>
                                    Id                                </th>
                                <td>
                                    {{ ticket.id }}
                                </td>
                            </tr>
                                                    <tr>
                                <th>
                                    Nom                                </th>
                                <td>
                                    {{ ticket.nom }}
                                </td>
                            </tr>
                                                    <tr>
                                <th>
                                    Valeur                                </th>
                                <td>
                                    {{ ticket.valeur }}
                                </td>
                            </tr>
                                            </tbody>
                </table>
            </div>
            <div class="panel-footer">
                <a href="{{ path('app_ticket_index') }}"><button class="btn btn-primary" title="retour Ã  la liste"><span class="fa fa-list"></span></button></a>
            </div>
        </div>
    </div>
</div>
{% endblock %}